$(build_xamlg_list): %.xaml.g.cs: %.xaml
	xamlg '$<'


$(ASSEMBLY_MDB): $(ASSEMBLY)
$(ASSEMBLY): $(build_sources) $(build_resources) $(build_datafiles) $(DLL_REFERENCES) $(PROJECT_REFERENCES) $(build_xamlg_list) $(build_satellite_assembly_list)
	make pre-all-local-hook prefix=$(prefix)
	mkdir -p $(shell dirname $(ASSEMBLY))
	make $(CONFIG)_BeforeBuild
	$(ASSEMBLY_COMPILER_COMMAND) $(ASSEMBLY_COMPILER_FLAGS) -out:$(ASSEMBLY) -target:$(COMPILE_TARGET) $(build_sources_embed) $(build_resources_embed) $(build_references_ref)
	make $(CONFIG)_AfterBuild
	make post-all-local-hook prefix=$(prefix)

install-local: $(ASSEMBLY) $(ASSEMBLY_MDB)
	make pre-install-local-hook prefix=$(prefix)
	make install-satellite-assemblies prefix=$(prefix)
	mkdir -p '$(DESTDIR)$(libdir)/$(PACKAGE)'
	$(call cp,$(ASSEMBLY),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call cp,$(ASSEMBLY_MDB),$(DESTDIR)$(libdir)/$(PACKAGE))
	mkdir -p '$(DESTDIR)$(libdir)/$(PACKAGE)/bin'
	$(call cp,$(MAGENTONETMVC_DLL_MDB),$(DESTDIR)$(libdir)/$(PACKAGE)/bin)
	$(call cp,$(SYSTEM_WEB_SERVICES_DLL),$(DESTDIR)$(libdir)/$(PACKAGE)/bin)
	$(call cp,$(SYSTEM_WEB_ROUTING_DLL),$(DESTDIR)$(libdir)/$(PACKAGE)/bin)
	$(call cp,$(SYSTEM_WEB_MVC_DLL),$(DESTDIR)$(libdir)/$(PACKAGE)/bin)
	$(call cp,$(SYSTEM_WEB_WEBPAGES_RAZOR_DLL),$(DESTDIR)$(libdir)/$(PACKAGE)/bin)
	$(call cp,$(SYSTEM_WEB_WEBPAGES_DLL),$(DESTDIR)$(libdir)/$(PACKAGE)/bin)
	mkdir -p '$(DESTDIR)$(libdir)/pkgconfig'
	$(call cp,$(MAGENTONETMVC_PC),$(DESTDIR)$(libdir)/pkgconfig)
	mkdir -p '$(DESTDIR)$(libdir)/$(PACKAGE)/Views'
	$(call cp,$(WEB_CONFIG),$(DESTDIR)$(libdir)/$(PACKAGE)/Views)
	mkdir -p '$(DESTDIR)$(libdir)/$(PACKAGE)/Views/Home'
	$(call cp,$(INDEX_CSHTML),$(DESTDIR)$(libdir)/$(PACKAGE)/Views/Home)
	mkdir -p '$(DESTDIR)$(libdir)/$(PACKAGE)/Views/Shared'
	$(call cp,$(LAYOUT_CSHTML),$(DESTDIR)$(libdir)/$(PACKAGE)/Views/Shared)
	$(call cp,$(ERROR_CSHTML),$(DESTDIR)$(libdir)/$(PACKAGE)/Views/Shared)
	$(call cp,$(VIEWSTART_CSHTML),$(DESTDIR)$(libdir)/$(PACKAGE)/Views)
	$(call cp,$(WEB_CONFIG2),$(DESTDIR)$(libdir)/$(PACKAGE))
	$(call cp,$(GLOBAL_ASAX),$(DESTDIR)$(libdir)/$(PACKAGE))
	mkdir -p '$(DESTDIR)$(libdir)/$(PACKAGE)/Content/js'
	$(call cp,$(BACKBONE_MIN_JS),$(DESTDIR)$(libdir)/$(PACKAGE)/Content/js)
	$(call cp,$(JQUERY_2_1_1_MIN_JS),$(DESTDIR)$(libdir)/$(PACKAGE)/Content/js)
	$(call cp,$(UNDERSCORE_MIN_JS),$(DESTDIR)$(libdir)/$(PACKAGE)/Content/js)
	$(call cp,$(BB_JS),$(DESTDIR)$(libdir)/$(PACKAGE)/Content/js)
	mkdir -p '$(DESTDIR)$(libdir)/$(PACKAGE)/Views/CatItem'
	$(call cp,$(INDEX_CSHTML2),$(DESTDIR)$(libdir)/$(PACKAGE)/Views/CatItem)
	$(call cp,$(CATITEM_INIT_JS),$(DESTDIR)$(libdir)/$(PACKAGE)/Content/js)
	mkdir -p '$(DESTDIR)$(libdir)/$(PACKAGE)/Content/css'
	$(call cp,$(BOOTSTRAP_THEME_CSS),$(DESTDIR)$(libdir)/$(PACKAGE)/Content/css)
	$(call cp,$(BOOTSTRAP_THEME_MIN_CSS),$(DESTDIR)$(libdir)/$(PACKAGE)/Content/css)
	$(call cp,$(BOOTSTRAP_CSS),$(DESTDIR)$(libdir)/$(PACKAGE)/Content/css)
	$(call cp,$(BOOTSTRAP_MIN_CSS),$(DESTDIR)$(libdir)/$(PACKAGE)/Content/css)
	mkdir -p '$(DESTDIR)$(libdir)/$(PACKAGE)/Content/fonts'
	$(call cp,$(GLYPHICONS_HALFLINGS_REGULAR_EOT),$(DESTDIR)$(libdir)/$(PACKAGE)/Content/fonts)
	$(call cp,$(GLYPHICONS_HALFLINGS_REGULAR_SVG),$(DESTDIR)$(libdir)/$(PACKAGE)/Content/fonts)
	$(call cp,$(GLYPHICONS_HALFLINGS_REGULAR_TTF),$(DESTDIR)$(libdir)/$(PACKAGE)/Content/fonts)
	$(call cp,$(GLYPHICONS_HALFLINGS_REGULAR_WOFF),$(DESTDIR)$(libdir)/$(PACKAGE)/Content/fonts)
	$(call cp,$(BOOTSTRAP_JS),$(DESTDIR)$(libdir)/$(PACKAGE)/Content/js)
	$(call cp,$(BOOTSTRAP_MIN_JS),$(DESTDIR)$(libdir)/$(PACKAGE)/Content/js)
	$(call cp,$(LAYOUT_BOOTSTRAP_CSHTML),$(DESTDIR)$(libdir)/$(PACKAGE)/Views/Shared)
	$(call cp,$(LAYOUT_BOOTSTRAP_BACKBONE_CSHTML),$(DESTDIR)$(libdir)/$(PACKAGE)/Views/Shared)
	$(call cp,$(HEADER_CSHTML),$(DESTDIR)$(libdir)/$(PACKAGE)/Views/Shared)
	$(call cp,$(STARTER_TEMPLATE_CSS),$(DESTDIR)$(libdir)/$(PACKAGE)/Content/css)
	$(call cp,$(STICKY_FOOTER_NAVBAR_CSS),$(DESTDIR)$(libdir)/$(PACKAGE)/Content/css)

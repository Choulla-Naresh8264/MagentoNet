@{Layout="~/Views/Shared/_Layout_bootstrap.cshtml";}
@model MagentoNetMvc.Models.CatItem
    <div class="row">
        <div class="page-header">
            <h1 id="navbar">Magento category Soap Calls</h1>
        </div>
        <div id="cats" class="col-lg-12">
          	<p>
                <h1>results of the magento SOAP call...</h1>
        	  	sessionid: @ViewBag.sessionId<br />
                name: @Html.DisplayTextFor(model => model.Name)<br />
        	  	desc: @Html.DisplayTextFor(model => model.Description)<br />
        	</p>
        </div>
    </div>
<script type="text/javascript">
 
  $( document ).ready(function() {
      $("ul.nav li").removeClass("active");
      $("ul.nav li#menu2").addClass("active");
  });
 
</script>
